{
  "name": "vue-notus",
  "version": "1.1.0",
  "description": "Vue Notus - A free Tailwind CSS and VueJS UI Kit and Admin by Creative Tim.",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "typecheck": "vue-tsc --noEmit",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.9.1",
    "@tailwindcss/forms": "^0.2.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/tsconfig": "^0.1.3",
    "chart.js": "^2.9.4",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "^1.0.9",
    "tailwindcss": "^2.0.4",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.5.0",
    "postcss": "^8.4.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
